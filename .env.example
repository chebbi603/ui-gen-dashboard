# Backend API base URL used by Vite dev proxy and runtime fetches
# Example: http://localhost:3000
VITE_API_BASE_URL=

# Optional request timeout (milliseconds) for API calls
VITE_API_TIMEOUT_MS=10000

# Optional WebSocket URL for real-time features (used by dev proxy)
# If not set, derived from API base (http->ws)
VITE_WS_URL=

# Optional static API token for dev-only use
# If unset, the app falls back to "dev-token-123" automatically
VITE_API_TOKEN=